(function(){
  var $routeList = $("#route-list");
  $routeList.effect("shake", {times: 1, distance:2});

  $("#stops-list").empty();
  $("#departures-list").empty();

  $routeList.empty();
  <% @routes.each do |route| %>
    $routeList.append("<%= j(render partial: 'shared/route', locals: { route: route, agency: @agency } )%> ");
  <% end %>
})();
